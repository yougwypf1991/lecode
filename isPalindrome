class Solution:
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        x_str = str(x)
        reverse_str = x_str[::-1]
        if reverse_str == x_str:
            return True
        else:
            return False
        
        
if __name__ == '__main__':
    s = Solution()
    print(s.isPalindrome(-121))
